# ROS-Install File

# Drag and Bot common - dependency for Pylon ros driver
- git: {local-name: dragandbot_common, uri: 'https://github.com/dragandbot/dragandbot_common.git', version: master}

# Pylon ROS Camera - camera driver
- git: {local-name: pylon-ros-camera, uri: 'https://github.com/basler/pylon-ros-camera.git', version: master}

# ROS OpcUa Communication - opcua libraries
- git: {local-name: ros_opcua_communication, uri: 'https://github.com/iirob/ros_opcua_communication.git', version: kinetic-devel}

# Industrial Calibration
- git: {local-name: industrial_calibration, uri: 'https://github.com/drchrislewis/industrial_calibration.git', version: target_tracker}

# Intelligent Actuator (Calibration dependency)
- git: {local-name: intelligent_actuator, uri: 'https://github.com/ros-industrial/intelligent_actuator.git', version: master}
